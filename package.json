    "scripts": {
        "start": "export NODE_ENV=dev && nodemon src/app.js",
        "start_prod": "export NODE_ENV=production && nodemon src/app.js",
        "test": "export NODE_ENV=test && jest --passWithNoTests --forceExit",
        "lint": "eslint --ignore-path .gitignore .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
        "validate": "yarn format && yarn lint",
        "validate-staged": "lint-staged",
        "postinstall": "husky install && husky add .husky/pre-commit \"yarn validate\" && husky add .husky/pre-push \"yarn test\""
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "config": "^3.3.7",
