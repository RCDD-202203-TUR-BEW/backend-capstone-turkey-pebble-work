        "encrypt-cookie": "^1.1.5",
        "error-handler": "^1.0.0",
        "express": "^4.17.2",
        "express-jwt": "^7.7.5",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "moment": "^2.29.4",
        "mongodb": "^4.8.0",
        "mongoose": "^6.4.6",
        "morgan": "^1.10.0",
        "nodemailer": "^6.7.7",
        "passport": "^0.6.0",
